{
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/predict",
      "dest": "app/main.py"
    },
    {
      "src": "/",
      "dest": "app/main.py"
    },
    {
      "src": "/docs",
      "dest": "app/main.py"
    }
  ]
}
